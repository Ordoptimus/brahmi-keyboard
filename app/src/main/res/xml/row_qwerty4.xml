<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <Row
        latin:keyWidth="10%p"
    >
        <Key
            latin:keyStyle="toSymbolKeyStyle"
            latin:keyWidth="15%p" />
        <!-- U+1104D: "ð‘" BRAMHI PUNCTUATION LOTUS
             U+1104C: "ð‘Œ" BRAMHI PUNCTUATION CRESCENT BAR
             U+1104B: "ð‘‹" BRAMHI PUNCTUATION LINE
             U+1104A: "ð‘Š" BRAMHI PUNCTUATION DOUBLE DOT
             U+11049: "ð‘‰" BRAMHI PUNCTUATION DOT -->
        <Key
            latin:keySpec="&#x1104D;"
            latin:moreKeys="&#x1104C;,&#x1104B;,&#x1104A;,&#x11049;,%" />
        <include
            latin:keyXPos="25%p"
            latin:keyboardLayout="@xml/key_space_5kw" />
        <!-- U+11047: "ð‘‡" BRAMHI DANDA
             U+11048: "ð‘ˆ" BRAMHI DOUBLE DANDA -->
        <Key
            latin:keySpec="&#x11047;"
            latin:moreKeys="&#x11048;,%" />
        <Key
            latin:keyStyle="enterKeyStyle"
            latin:keyWidth="fillRight" />
    </Row>
</merge>
